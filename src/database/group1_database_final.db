BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS "employee" (
	"employeeID"	INTEGER,
	"name"	TEXT,
	"surname"	TEXT,
	"age"	INTEGER,
	"stationID"	INTEGER,
	PRIMARY KEY("employeeID")
);
CREATE TABLE IF NOT EXISTS "senior_resident" (
	"resID"	INTEGER,
	"name"	TEXT,
	"surname"	TEXTe,
	"age"	INTEGER,
	"room"	INTEGER,
	"stationID"	INTEGER,
	"visitID"	INTEGER,
	"iceID"	INTEGER,
	PRIMARY KEY("resID")
);
CREATE TABLE IF NOT EXISTS "station" (
	"stationID"	INTEGER,
	"name"	TEXT,
	PRIMARY KEY("stationID")
);
CREATE TABLE IF NOT EXISTS "ice" (
	"iceID"	INTEGER,
	"resID"	INTEGER,
	"name"	TEXT,
	"surname"	TEXT,
	"tel_number"	BLOB,
	"adress"	BLOB,
	PRIMARY KEY("iceID")
);
CREATE TABLE IF NOT EXISTS "visits" (
	"visitID"	INTEGER,
	"resID"	INTEGER,
	"description"	TEXT,
	PRIMARY KEY("visitID")
);
CREATE TABLE IF NOT EXISTS "shift_shedule" (
	"shiftID"	INTEGER,
	"employeeID"	INTEGER,
	"date"	BLOB,
	"category"	INTEGER,
	PRIMARY KEY("shiftID")
);
CREATE TABLE IF NOT EXISTS "incidents" (
	"incidentID"	INTEGER,
	"description"	TEXT,
	"resID"	INTEGER,
	"shiftID"	INTEGER,
	PRIMARY KEY("incidentID")
);
CREATE TABLE IF NOT EXISTS "medication" (
	"medicID"	INTEGER,
	"name"	TEXT,
	PRIMARY KEY("medicID")
);
CREATE TABLE IF NOT EXISTS "medplan" (
	"medID"	INTEGER,
	"intake_frequency"	REAL,
	"concentration"	REAL,
	"resID"	INTEGER,
	"medicID"	INTEGER,
	PRIMARY KEY("medID")
);
